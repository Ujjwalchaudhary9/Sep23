<%- include('./base/header.ejs') %>
    <div class="container">
        <div class="row">
            <table class="table-text-center">
                <h3 class="text-center"> Student List</h3>
                <thead>
                    <tr>
                        <th>Rollno</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>father Name</th>
                        <th>Aadhar Card</th>
                        <th>mobileNo</th>
                    </tr>
                </thead>
                <% students.forEach((student)=> { %>
                    <tr>
                        <td>
                            <%=student.rollno %>
                        </td>
                        <td>
                            <%=student.firstName %>
                        </td>
                        <td>
                            <%=student.lastName %>
                        </td>
                        <td>
                            <%=student.fatherName %>
                        </td>
                        <td>
                            <%=student.adharCardNo %>
                        </td>
                        <td>
                            <%=student.mobileNo %>
                        </td>
                    </tr>

                    <% }) %>
            </table>
        </div>
    </div>
    <%- include('./base/footer.ejs') %>